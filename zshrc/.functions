function plrb () {
    git pull --rebase origin `git rev-parse --abbrev-ref HEAD`
}

function rzsh () {
    source $HOME/.zshrc
}

function open_kintaiman () {
  ID=`cat ~/Documents/GitProjects/kintaiman_v2/gapps.config.json | jq -r .fileId`
  open "https://script.google.com/a/grooves.co.jp/d/${ID}/edit" -a 'Google Chrome'
}
